{
  "name": "@navikt/virksomhetsvelger",
  "private": false,
  "version": "1.0.1-rc",
  "type": "module",
  "main": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "default": "./dist/main.js"
    },
    "./dist/*.css": {
      "import": "./dist/*.css",
      "require": "./dist/*.css"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/virksomhetsvelger"
  },
  "scripts": {
    "dev": "vite",
    "dev:watch": "run-p build:watch dev",
    "build": "tsc -b ./tsconfig.lib.json && vite build",
    "build:watch": "tsc -b ./tsconfig.lib.json && vite build --watch",
    "build:cicd": "run-s lint test build",
    "build:demo": "vite build -c vite.config.demo.ts",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest run"
  },
  "peerDependencies": {
    "@navikt/ds-css": ">=6",
    "@navikt/ds-react": ">=6",
    "react": "18 || 19",
    "react-dom": "18 || 19"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@faker-js/faker": "^9.7.0",
    "@navikt/aksel-icons": "^7.21.0",
    "@navikt/ds-css": "^7.21.0",
    "@navikt/ds-react": "^7.21.0",
    "@navikt/nav-dekoratoren-moduler": "3.2.3",
    "@types/node": "22.15.3",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "eslint": "^9.26.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.31.1",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "3.1.2"
  },
  "dependencies": {
    "focus-trap-react": "11.0.3"
  }
}
